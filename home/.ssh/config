	AddKeysToAgent yes
	IdentityFile ~/.ssh/github/id_ed25519

# Personal GitLab instance
Host gitlab.com
	AddKeysToAgent yes
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/gitlab/Personal/id_ed25519

# Enterprise GitLab instance
Host gitlab.ewi.tudelft.nl
	AddKeysToAgent yes
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/gitlab/Enterprise/id_ed25519
